{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
        "max-line-length": false,
        "no-trailing-whitespace": false,
        "eofline": false,
        "comment-format": false
    }
}


// tslint:disable:max-line-length
// tslint:disable:no-trailing-whitespace
